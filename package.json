{
  "name": "abai-desktop",
  "version": "1.3.0",
  "description": "A-B Test with any AI model",
  "packageManager": "npm@10.0.0",
  "main": "apps/main/dist/main/index.js",
  "scripts": {
    "dev": "concurrently -k -n UI,MAIN \"npm run dev --prefix apps/ui\" \"cross-env VITE_DEV_SERVER_URL=http://localhost:5174 npm run dev --prefix apps/main\"",
    "build": "turbo run build",
    "build:fresh": "turbo run build --force",
    "start:prod": "cd apps/main && npm run start",
    "dev:prod": "npm run build:fresh && npm run start:prod",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:webhooks": "node test-stripe-webhooks.js",
    "test:production": "node test-production-integration.js",
    "test:packaged": "node test-packaged-app.js",
    "test:all": "npm run test && npm run test:webhooks && npm run test:production",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky",
    "docs": "start docs/first-model.md || open docs/first-model.md || xdg-open docs/first-model.md",
    "package": "npm run build:fresh && electron-builder --publish never",
    "package:win": "npm run build:fresh && electron-builder --win --x64 --publish never",
    "package:win:npm": "npm run build:fresh && cd apps/main && npm install --production --force && cd ../.. && electron-builder --win --x64 --publish never",
    "package:mac": "npm run build:fresh && electron-builder --mac --arm64 --x64 --publish never"
  },
  "keywords": [],
  "author": {
    "name": "Skylord afk",
    "email": "ssdfsdfasdfa@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@playwright/test": "^1.42.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.25",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitest/ui": "^1.4.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "electron": "^29.1.5",
    "electron-builder": "^24.13.3",
    "eslint": "9.29.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fastify": "^4.26.2",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.2",
    "turbo": "^2.5.4",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.23",
    "@ai-sdk/google": "^0.0.20",
    "@ai-sdk/openai": "^0.0.33",
    "@ai-sdk/react": "^0.0.12",
    "@dqbd/tiktoken": "^1.0.13",
    "@sentry/electron": "^6.8.0",
    "@stripe/stripe-js": "^3.5.0",
    "ai": "^3.2.16",
    "axios": "^1.7.2",
    "better-sqlite3": "^9.4.3",
    "electron-store": "^8.1.0",
    "react-hot-toast": "^2.4.1",
    "stripe": "^16.2.0",
    "uuid": "^9.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "workspaces": [
    "apps/main",
    "apps/ui"
  ]
}
